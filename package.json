{
  "name": "sls-template",
  "version": "1.0.0",
  "description": "Serverless template",
  "scripts": {
    "dev": "cross-env NODE_ENV=development sls offline start --stage offline",
    "deploy-prod": "cross-env NODE_ENV=production sls deploy --stage production",
    "deploy-function": "cross-env NODE_ENV=production sls deploy function --stage production --function",
    "remove-prod": "sls remove --stage production",
    "build-types": "tsc --noEmit --pretty"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "serverless": "^3.0.1",
    "serverless-bundle": "^5.2.0",
    "serverless-offline": "^8.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "serverless-http": "^2.7.0"
  },
  "author": "dhruvsaxena <https://github.com/dhruvsaxena1998>"
}
