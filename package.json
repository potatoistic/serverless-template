{
  "name": "template-serverless",
  "version": "1.0.0",
  "description": "Serverless template",
  "scripts": {
    "dev": "cross-env NODE_ENV=development sls offline start --stage dev",
    "deploy-prod": "cross-env NODE_ENV=production sls deploy --stage prod",
    "deploy-function": "cross-env NODE_ENV=production sls deploy function --stage prod --function",
    "remove-prod": "sls remove --stage prod",
    "build-types": "tsc --noEmit --pretty"
  },
  "devDependencies": {
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "serverless": "^3.0.1",
    "serverless-bundle": "^5.2.0",
    "serverless-offline": "^8.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "aws-lambda-fastify": "^2.1.0",
    "fastify": "^3.27.2",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^7.0.1"
  },
  "author": "dhruvsaxena <https://github.com/dhruvsaxena1998>"
}
